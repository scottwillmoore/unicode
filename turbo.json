{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**"]
		},
		"clean": {
			"cache": false
		},
		"develop": {
			"cache": false,
			"persistent": true
		},
		"preview": {
			"cache": false,
			"dependsOn": ["^build"],
			"persistent": false
		}
	}
}
